      "source": [
        "# 商品価格と税率をもとに税込金額を計算する関数\n",
        "def calc_tax(price, tax_rate):\n",
        "    total = price + (price * tax_rate / 100)\n",
        "    return total\n",
        "\n",
        "# 例：商品の価格が110円、消費税率が10%\n",
        "result = calc_tax(110, 10)\n",
        "\n",
        "# 計算結果を表示\n",
        "print(f\"税込金額は {result} 円です\")\n"
      ]
    }
