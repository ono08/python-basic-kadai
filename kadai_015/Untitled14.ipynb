        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    def printinfo(self):\n",
        "        print(f\"名前: {self.name}\")\n",
        "        print(f\"年齢: {self.age}歳\")\n",
        "\n",
        "# インスタンスを作成して使う\n",
        "person = Human(\"田中太郎\", 30)\n",
        "person.printinfo()\n"
      ]
    }
  ]
